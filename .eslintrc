{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "no-console": 0,
    "indent": [2, 2],
    "quotes": [2, "single", {"avoidEscape": true, "allowTemplateLiterals": true}],
    "max-len": [2, 130]
  },
  "plugins": [
    "react"
  ]
}
